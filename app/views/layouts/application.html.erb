<!DOCTYPE html>
<html>
  <head>
    <title>Fitnessapp</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%= stylesheet_link_tag "tailwind", "inter-font", "data-turbo-track": "reload" %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
    <script src="https://unpkg.com/scrollreveal"></script>
  </head>

  <body>
    <div>
<div class="antialiased bg-gray-100">
<div class="w-full text-gray-700 bg-white">
<div class="flex flex-col max-w-screen-xl px-4 mx-auto md:items-center md:justify-between md:flex-row md:px-6 lg:px-8">
<div class="flex flex-row items-center justify-between p-4">
<%=link_to "fitnessapp", root_path, class: ' bg-gray-100 text-lg font-semibold tracking-widest text-gray-900 uppercase rounded-lg focus: outline-none focus: shadow-outline' %>
</div>
<nav class=" bg-gray-100 flex-col flex-grow hidden pb-4 md:pb-0 md:flex md:justify-end md:flex-row">
<%nav_item_class="px-4 py-2 mt-2 text-sm font-semibold bg-transparent rounded-lg md:mt-0 md:ml-4 hover:text-gray-900 focus:text-gray-900 hover:bg-gray-200 focus:bg-gray-20 focus:outline-none fous: shadow-outline" %>
<% if user_signed_in? %>
<%=link_to "ワークアウト投稿", new_daily_workout_path, class: nav_item_class %>
<%=link_to "マイプロフィール", profiles_show_path, class: nav_item_class %>
<%=link_to "ワークアウト管理", new_scadule_path, class: nav_item_class %>
<%=link_to 'ワークスケジュール', pages_home_path, class: nav_item_class%>
<%= link_to "ログアウト", destroy_user_session_path, class: nav_item_class, method: :delete %>
<% else %>
<%= link_to "新規登録", new_user_registration_path, class: nav_item_class %>
<%=link_to 'ログイン', new_user_session_path, class: nav_item_class%>
<% end %>
</nav>
</div>
</div>
</div>
</div>
<% flash.each do |msg_type, msg|%>
<div class="bg-green-500">
<div class="max-w-7xl mx-auto py-3 px-3 sm:px-6 lg:px-8">
<div class="flex items-center justify-between flex-wrap">
<div class="w-8 flex-1 flex items-center">
<p class="ml-3 font-medium text-white truncate">
<%=msg%>
</p>
</div>
</div>
</div>
</div>
<%end%>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>

   <%= yield %>
 
